{
  "name": "el-conversor",
  "version": "1.0.0",
  "description": "A number to word list converter as a Node backend and React/Redux fronted",
  "main": "index.js",
  "scripts": {
    "server:check": "npm run server:test && npm run server:lint",
    "server:dev": "NODE_ENV=dev nodemon --exec babel-node ./server/index.js --watch",
    "server:lint": "eslint --config=./server/.eslintrc.js \"./server/**(!test).js\"",
    "server:test": "nyc --reporter=text --reporter=html mocha --exit --compilers js:babel-core/register ./server/**/*.test.js",
    "start": "NODE_ENV=production pm2 start ./server/index.js --interpreter babel-node --no-daemon"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "t9"
  ],
  "author": "Daniel Caldas",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "express": "4.16.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.0",
    "mocha": "5.0.4",
    "nodemon": "1.17.2",
    "nyc": "11.6.0",
    "pm2": "2.10.1"
  },
  "engines": {
    "node": "8.9.0"
  }
}
