{
  "name": "el-conversor",
  "version": "1.0.0",
  "description": "A number to word list converter as a Node backend and React/Redux fronted",
  "main": "index.js",
  "scripts": {
    "client:dev": "NODE_ENV=dev webpack-dev-server --optimize-minimize -d --content-base app --config app/webpack.config.js --inline --hot --port 3002",
    "server:check": "npm run server:test && npm run server:lint",
    "server:dev": "NODE_ENV=dev nodemon --exec babel-node ./server/src --watch",
    "server:lint": "eslint --config=./server/.eslintrc.js \"./server/**(!test).js\"",
    "server:test": "nyc --reporter=text --reporter=html mocha --recursive --exit --compilers js:babel-core/register ./server/test",
    "server:test-watch": "nyc --reporter=html mocha --recursive --watch --compilers js:babel-core/register ./server/test",
    "start": "NODE_ENV=production pm2 start ./server/src --interpreter babel-node --no-daemon"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "t9"
  ],
  "author": "Daniel Caldas",
  "license": "ISC",
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-promise-middleware": "5.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-webpack-plugin": "3.0.7",
    "mocha": "5.0.4",
    "nodemon": "1.17.2",
    "nyc": "11.6.0",
    "pm2": "2.10.1",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.1.3",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "3.1.1"
  },
  "engines": {
    "node": "8.9.0"
  }
}
