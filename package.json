{
  "name": "el-conversor",
  "version": "1.0.0",
  "description": "A number to word list converter as a Node backend and React/Redux fronted",
  "scripts": {
    "check": "npm run server:check && npm run client:check",
    "client:check": "npm run client:lint && npm run client:test",
    "client:dev": "NODE_ENV=dev webpack-dev-server -d --content-base app --config app/webpack.config.js --inline --hot --port 3002",
    "client:dist": "NODE_ENV=production webpack --config app/webpack.config.js -p",
    "client:http-server": "http-server ./app/dist/ -p 8888 -c-1",
    "client:functional-local": "export CYPRESS_SANDBOX_URL=http://localhost:3002/ && cypress open",
    "client:functional-remote": "export CYPRESS_SANDBOX_URL=https://el-conversor.herokuapp.com/ && cypress open",
    "client:functional": "export CYPRESS_SANDBOX_URL=http://127.0.0.1:8888 && cypress run",
    "client:lint": "eslint --config=./app/.eslintrc.js \"./app/**(!test).js\"",
    "client:test-watch": "jest --config app/jest.config.js --verbose --coverage --watchAll",
    "client:test": "jest --config app/jest.config.js --verbose --coverage",
    "dev": "COMMIT_HASH=\"$(git rev-parse HEAD)\" npm-run-all --parallel *:dev",
    "server:check": "npm run server:test && npm run server:lint",
    "server:dev": "NODE_ENV=dev nodemon --exec babel-node ./server/src --watch",
    "server:lint": "eslint --config=./server/.eslintrc.js \"./server/**(!test).js\"",
    "server:test": "nyc --reporter=text --reporter=html mocha --recursive --exit --compilers js:babel-core/register ./server/test",
    "server:test-watch": "nyc --reporter=html mocha --recursive --watch --compilers js:babel-core/register ./server/test",
    "start": "COMMIT_HASH=\"$(git rev-parse HEAD)\" NODE_ENV=production pm2 start ./server/src --interpreter babel-node --no-daemon",
    "postinstall": "npm run client:dist"
  },
  "keywords": [
    "node",
    "express",
    "react",
    "t9"
  ],
  "author": "Daniel Caldas",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cors": "2.8.4",
    "express": "4.16.3",
    "jsonfile": "4.0.0",
    "pm2": "2.10.1",
    "react": "16.2.0",
    "react-dom": "^16.2.1",
    "react-redux": "5.0.7",
    "redux": "^4.0.1",
    "redux-logger": "3.0.6",
    "redux-promise": "0.5.3",
    "redux-promise-middleware": "5.0.0",
    "@cypress/webpack-preprocessor": "2.0.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-loader": "7.1.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-react-html-attrs": "2.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "chai": "4.1.2",
    "chai-http": "3.0.0",
    "css-loader": "0.28.11",
    "cypress": "2.1.0",
    "ejs-loader": "0.3.1",
    "eslint": "4.19.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.7.0",
    "eslint-plugin-standard": "3.0.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.11",
    "html-webpack-plugin": "3.0.7",
    "http-server": "0.11.1",
    "jest": "22.4.3",
    "mocha": "5.0.4",
    "node-sass": "4.7.2",
    "node-wget": "0.4.2",
    "nodemon": "1.17.2",
    "npm-run-all": "4.1.2",
    "nyc": "11.6.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.1.3",
    "remote-redux-devtools": "^0.5.16",
    "remotedev-server": "^0.3.1",
    "sass-loader": "6.0.7",
    "style-loader": "0.20.3",
    "webpack": "4.2.0",
    "webpack-cli": "2.0.12",
    "webpack-dev-server": "^3.1.11"
  }
}
